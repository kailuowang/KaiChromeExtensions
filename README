function loadScript(sScriptSrc) {
  var oHead = document.getElementsByTagName('head')[0];
  var oScript = document.createElement('script'); 
  oScript.type = 'text/javascript';
  oScript.src = sScriptSrc;
  oHead.appendChild(oScript);
} 
loadScript('http://code.jquery.com/jquery-1.7.2.min.js');
loadScript('http://documentcloud.github.com/underscore/underscore-min.js');
loadScript('https://github.com/kailuowang/KaiChromeExtensions/raw/master/retrieve.js');
